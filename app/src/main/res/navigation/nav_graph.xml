<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/templateFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.projektmb4pp.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_loginFragment_to_productGridFragment2"
            app:destination="@id/productGridFragment2"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="false" />
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment"
            app:enterAnim="@android:anim/fade_in" />
    </fragment>
    <fragment
        android:id="@+id/productGridFragment2"
        android:name="com.example.projektmb4pp.ProductGridFragment"
        android:label="fragment_product_gridd"
        tools:layout="@layout/fragment_product_gridd" />
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.projektmb4pp.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@android:anim/fade_in" />
    </fragment>
    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.example.projektmb4pp.WelcomeFragment"
        android:label="fragment_welcome"
        tools:layout="@layout/fragment_welcome" >
        <action
            android:id="@+id/action_welcomeFragment_to_productGridFragment2"
            app:destination="@id/productGridFragment2"
            app:enterAnim="@android:anim/fade_in" />
    </fragment>
    <fragment
        android:id="@+id/templateFragment"
        android:name="com.example.projektmb4pp.TemplateFragment"
        android:label="fragment_template"
        tools:layout="@layout/fragment_template" />
</navigation>